<?php

namespace Corporativo\ConcurseiroBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * CargoDisciplinaAssunto
 *
 * @ORM\Table(name="cargo_disciplina_assunto", indexes={@ORM\Index(name="idx_fk_assunto_cargo_disciplina_assunto", columns={"co_assunto"}), @ORM\Index(name="idx_fk_cargo_disciplina_cargo_disciplina_assunto", columns={"co_cargo_disciplina"})})
 * @ORM\Entity
 */
class CargoDisciplinaAssunto
{
    /**
     * @var integer
     *
     * @ORM\Column(name="co_cargo_disciplina_assunto", type="integer", nullable=false)
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="SEQUENCE")
     * @ORM\SequenceGenerator(sequenceName="cargo_disciplina_assunto_co_cargo_disciplina_assunto_seq", allocationSize=1, initialValue=1)
     */
    private $coCargoDisciplinaAssunto;

    /**
     * @var \Assunto
     *
     * @ORM\ManyToOne(targetEntity="Assunto")
     * @ORM\JoinColumns({
     *   @ORM\JoinColumn(name="co_assunto", referencedColumnName="co_assunto")
     * })
     */
    private $coAssunto;

    /**
     * @var \CargoDisciplina
     *
     * @ORM\ManyToOne(targetEntity="CargoDisciplina")
     * @ORM\JoinColumns({
     *   @ORM\JoinColumn(name="co_cargo_disciplina", referencedColumnName="co_cargo_disciplina")
     * })
     */
    private $coCargoDisciplina;


}
