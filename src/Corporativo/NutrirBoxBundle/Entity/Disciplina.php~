<?php

namespace Corporativo\ConcurseiroBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Disciplina
 *
 * @ORM\Table(name="disciplina", uniqueConstraints={@ORM\UniqueConstraint(name="idx_pk_disciplina", columns={"co_disciplina"})})
 * @ORM\Entity
 */
class Disciplina
{
    /**
     * @var integer
     *
     * @ORM\Column(name="co_disciplina", type="integer", nullable=false)
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="SEQUENCE")
     * @ORM\SequenceGenerator(sequenceName="disciplina_co_disciplina_seq", allocationSize=1, initialValue=1)
     */
    private $coDisciplina;

    /**
     * @var string
     *
     * @ORM\Column(name="no_nome", type="string", length=300, nullable=false)
     */
    private $noNome;

    /**
     * @var string
     *
     * @ORM\Column(name="de_descricao", type="text", nullable=true)
     */
    private $deDescricao;

    /**
     * @var string
     *
     * @ORM\Column(name="no_cor", type="string", length=50, nullable=true)
     */
    private $noCor;


}
