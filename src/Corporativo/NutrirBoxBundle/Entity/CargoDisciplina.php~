<?php

namespace Corporativo\ConcurseiroBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * CargoDisciplina
 *
 * @ORM\Table(name="cargo_disciplina", uniqueConstraints={@ORM\UniqueConstraint(name="idx_pk_cargo_disciplina", columns={"co_cargo_disciplina"})}, indexes={@ORM\Index(name="idx_fk_disciplina_cargo_disciplina", columns={"co_disciplina"}), @ORM\Index(name="idx_fk_cargo_cargo_disciplina", columns={"co_cargo"})})
 * @ORM\Entity
 */
class CargoDisciplina
{
    /**
     * @var integer
     *
     * @ORM\Column(name="co_cargo_disciplina", type="integer", nullable=false)
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="SEQUENCE")
     * @ORM\SequenceGenerator(sequenceName="cargo_disciplina_co_cargo_disciplina_seq", allocationSize=1, initialValue=1)
     */
    private $coCargoDisciplina;

    /**
     * @var integer
     *
     * @ORM\Column(name="nu_peso", type="smallint", nullable=true)
     */
    private $nuPeso;

    /**
     * @var integer
     *
     * @ORM\Column(name="nu_qtd_questao", type="smallint", nullable=true)
     */
    private $nuQtdQuestao;

    /**
     * @var \Cargo
     *
     * @ORM\ManyToOne(targetEntity="Cargo")
     * @ORM\JoinColumns({
     *   @ORM\JoinColumn(name="co_cargo", referencedColumnName="co_cargo")
     * })
     */
    private $coCargo;

    /**
     * @var \Disciplina
     *
     * @ORM\ManyToOne(targetEntity="Disciplina")
     * @ORM\JoinColumns({
     *   @ORM\JoinColumn(name="co_disciplina", referencedColumnName="co_disciplina")
     * })
     */
    private $coDisciplina;


}
