<?php

namespace Corporativo\ConcurseiroBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Banca
 *
 * @ORM\Table(name="banca", uniqueConstraints={@ORM\UniqueConstraint(name="idx_pk_banca", columns={"co_banca"})})
 * @ORM\Entity
 */
class Banca
{
    /**
     * @var integer
     *
     * @ORM\Column(name="co_banca", type="integer", nullable=false)
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="SEQUENCE")
     * @ORM\SequenceGenerator(sequenceName="banca_co_banca_seq", allocationSize=1, initialValue=1)
     */
    private $coBanca;

    /**
     * @var string
     *
     * @ORM\Column(name="no_nome", type="string", length=300, nullable=false)
     */
    private $noNome;

    /**
     * @var string
     *
     * @ORM\Column(name="de_descricao", type="text", nullable=true)
     */
    private $deDescricao;

    /**
     * @var string
     *
     * @ORM\Column(name="no_link", type="string", length=300, nullable=true)
     */
    private $noLink;

    /**
     * @var boolean
     *
     * @ORM\Column(name="is_ativo", type="boolean", nullable=false)
     */
    private $isAtivo;


}
