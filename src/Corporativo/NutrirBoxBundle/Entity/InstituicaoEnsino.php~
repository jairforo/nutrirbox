<?php

namespace Corporativo\ConcurseiroBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * InstituicaoEnsino
 *
 * @ORM\Table(name="instituicao_ensino", uniqueConstraints={@ORM\UniqueConstraint(name="idx_pk_instituicao_ensino", columns={"co_instituicao_ensino"})})
 * @ORM\Entity
 */
class InstituicaoEnsino
{
    /**
     * @var integer
     *
     * @ORM\Column(name="co_instituicao_ensino", type="integer", nullable=false)
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="SEQUENCE")
     * @ORM\SequenceGenerator(sequenceName="instituicao_ensino_co_instituicao_ensino_seq", allocationSize=1, initialValue=1)
     */
    private $coInstituicaoEnsino;

    /**
     * @var string
     *
     * @ORM\Column(name="no_instituicao_ensino", type="string", length=50, nullable=true)
     */
    private $noInstituicaoEnsino;

    /**
     * @var boolean
     *
     * @ORM\Column(name="is_ativo", type="boolean", nullable=false)
     */
    private $isAtivo;


}
