<?php

namespace Corporativo\ConcurseiroBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * ConhecimentoDisciplina
 *
 * @ORM\Table(name="conhecimento_disciplina", uniqueConstraints={@ORM\UniqueConstraint(name="idx_pk_conhecimento_disciplina", columns={"co_conhecimento_discplina"})}, indexes={@ORM\Index(name="idx_fk_planejamento_estudo_conhecimento_disciplina", columns={"co_planejamento"}), @ORM\Index(name="idx_fk_disciplina_conhecimento_disciplina", columns={"co_disciplina"})})
 * @ORM\Entity
 */
class ConhecimentoDisciplina
{
    /**
     * @var integer
     *
     * @ORM\Column(name="co_conhecimento_discplina", type="integer", nullable=false)
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="SEQUENCE")
     * @ORM\SequenceGenerator(sequenceName="conhecimento_disciplina_co_conhecimento_discplina_seq", allocationSize=1, initialValue=1)
     */
    private $coConhecimentoDiscplina;

    /**
     * @var integer
     *
     * @ORM\Column(name="nu_nivel_conhecimento", type="smallint", nullable=false)
     */
    private $nuNivelConhecimento;

    /**
     * @var \Disciplina
     *
     * @ORM\ManyToOne(targetEntity="Disciplina")
     * @ORM\JoinColumns({
     *   @ORM\JoinColumn(name="co_disciplina", referencedColumnName="co_disciplina")
     * })
     */
    private $coDisciplina;

    /**
     * @var \PlanejamentoEstudo
     *
     * @ORM\ManyToOne(targetEntity="PlanejamentoEstudo")
     * @ORM\JoinColumns({
     *   @ORM\JoinColumn(name="co_planejamento", referencedColumnName="co_planejamento")
     * })
     */
    private $coPlanejamento;


}
