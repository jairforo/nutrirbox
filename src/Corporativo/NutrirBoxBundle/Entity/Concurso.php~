<?php

namespace Corporativo\ConcurseiroBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Concurso
 *
 * @ORM\Table(name="concurso", indexes={@ORM\Index(name="idx_fk_banca_concurso", columns={"co_banca"}), @ORM\Index(name="idx_fk_referencia_valor_concurso", columns={"co_referencia_valor"})})
 * @ORM\Entity
 */
class Concurso
{
    /**
     * @var integer
     *
     * @ORM\Column(name="co_concurso", type="integer", nullable=false)
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="SEQUENCE")
     * @ORM\SequenceGenerator(sequenceName="concurso_co_concurso_seq", allocationSize=1, initialValue=1)
     */
    private $coConcurso;

    /**
     * @var string
     *
     * @ORM\Column(name="no_nome", type="string", length=300, nullable=false)
     */
    private $noNome;

    /**
     * @var string
     *
     * @ORM\Column(name="de_descricao", type="text", nullable=true)
     */
    private $deDescricao;

    /**
     * @var string
     *
     * @ORM\Column(name="no_link_edital", type="string", length=300, nullable=true)
     */
    private $noLinkEdital;

    /**
     * @var \DateTime
     *
     * @ORM\Column(name="dt_abertura_inscricao", type="date", nullable=true)
     */
    private $dtAberturaInscricao;

    /**
     * @var \DateTime
     *
     * @ORM\Column(name="dt_final_inscricao", type="date", nullable=true)
     */
    private $dtFinalInscricao;

    /**
     * @var \DateTime
     *
     * @ORM\Column(name="dt_prova", type="date", nullable=true)
     */
    private $dtProva;

    /**
     * @var boolean
     *
     * @ORM\Column(name="is_ativo", type="boolean", nullable=false)
     */
    private $isAtivo;

    /**
     * @var \Banca
     *
     * @ORM\ManyToOne(targetEntity="Banca")
     * @ORM\JoinColumns({
     *   @ORM\JoinColumn(name="co_banca", referencedColumnName="co_banca")
     * })
     */
    private $coBanca;

    /**
     * @var \ReferenciaValor
     *
     * @ORM\ManyToOne(targetEntity="ReferenciaValor")
     * @ORM\JoinColumns({
     *   @ORM\JoinColumn(name="co_referencia_valor", referencedColumnName="co_referencia_valor")
     * })
     */
    private $coReferenciaValor;


}
