<?php

namespace Corporativo\ConcurseiroBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * TipoEstudo
 *
 * @ORM\Table(name="tipo_estudo", uniqueConstraints={@ORM\UniqueConstraint(name="idx_pk_tipo_estudo", columns={"co_tipo_estudo"})})
 * @ORM\Entity
 */
class TipoEstudo
{
    /**
     * @var integer
     *
     * @ORM\Column(name="co_tipo_estudo", type="integer", nullable=false)
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="SEQUENCE")
     * @ORM\SequenceGenerator(sequenceName="tipo_estudo_co_tipo_estudo_seq", allocationSize=1, initialValue=1)
     */
    private $coTipoEstudo;

    /**
     * @var string
     *
     * @ORM\Column(name="no_nome", type="string", length=300, nullable=false)
     */
    private $noNome;

    /**
     * @var string
     *
     * @ORM\Column(name="de_descricao", type="text", nullable=true)
     */
    private $deDescricao;

    /**
     * @var integer
     *
     * @ORM\Column(name="nu_percentual", type="smallint", nullable=false)
     */
    private $nuPercentual;


}
