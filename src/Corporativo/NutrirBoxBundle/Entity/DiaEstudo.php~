<?php

namespace Corporativo\ConcurseiroBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * DiaEstudo
 *
 * @ORM\Table(name="dia_estudo", uniqueConstraints={@ORM\UniqueConstraint(name="idx_pk_dia_estudo", columns={"dt_dia", "co_planejamento"})}, indexes={@ORM\Index(name="idx_fk_planejamento_estudo_dia_estudo", columns={"co_planejamento"})})
 * @ORM\Entity
 */
class DiaEstudo
{
    /**
     * @var \DateTime
     *
     * @ORM\Column(name="dt_dia", type="date", nullable=false)
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="NONE")
     */
    private $dtDia;

    /**
     * @var \PlanejamentoEstudo
     *
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="NONE")
     * @ORM\OneToOne(targetEntity="PlanejamentoEstudo")
     * @ORM\JoinColumns({
     *   @ORM\JoinColumn(name="co_planejamento", referencedColumnName="co_planejamento")
     * })
     */
    private $coPlanejamento;


}
