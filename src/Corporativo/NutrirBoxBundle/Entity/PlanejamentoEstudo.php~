<?php

namespace Corporativo\ConcurseiroBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * PlanejamentoEstudo
 *
 * @ORM\Table(name="planejamento_estudo", uniqueConstraints={@ORM\UniqueConstraint(name="idx_pk_planejamento_estudo", columns={"co_planejamento_estudo"})}, indexes={@ORM\Index(name="idx_fk_planejamento_semanal_planejamento_estudo", columns={"co_planejamento_semanal"}), @ORM\Index(name="IDX_EDBB024025BDB3CC", columns={"co_transacao"}), @ORM\Index(name="IDX_EDBB024046AC5E73", columns={"co_cargo"}), @ORM\Index(name="IDX_EDBB0240F2AC88A7", columns={"co_pessoa"})})
 * @ORM\Entity
 */
class PlanejamentoEstudo
{
    /**
     * @var integer
     *
     * @ORM\Column(name="co_planejamento_estudo", type="integer", nullable=false)
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="SEQUENCE")
     * @ORM\SequenceGenerator(sequenceName="planejamento_estudo_co_planejamento_estudo_seq", allocationSize=1, initialValue=1)
     */
    private $coPlanejamentoEstudo;

    /**
     * @var \DateTime
     *
     * @ORM\Column(name="dt_cadastro", type="date", nullable=false)
     */
    private $dtCadastro;

    /**
     * @var \DateTime
     *
     * @ORM\Column(name="dt_inicio", type="date", nullable=false)
     */
    private $dtInicio;

    /**
     * @var string
     *
     * @ORM\Column(name="no_nome", type="string", length=300, nullable=false)
     */
    private $noNome;

    /**
     * @var boolean
     *
     * @ORM\Column(name="is_ativo", type="boolean", nullable=false)
     */
    private $isAtivo;

    /**
     * @var \PlanejamentoSemanal
     *
     * @ORM\ManyToOne(targetEntity="PlanejamentoSemanal")
     * @ORM\JoinColumns({
     *   @ORM\JoinColumn(name="co_planejamento_semanal", referencedColumnName="co_planejamento_semanal")
     * })
     */
    private $coPlanejamentoSemanal;

    /**
     * @var \Transacao
     *
     * @ORM\ManyToOne(targetEntity="Transacao")
     * @ORM\JoinColumns({
     *   @ORM\JoinColumn(name="co_transacao", referencedColumnName="co_transacao")
     * })
     */
    private $coTransacao;

    /**
     * @var \Cargo
     *
     * @ORM\ManyToOne(targetEntity="Cargo")
     * @ORM\JoinColumns({
     *   @ORM\JoinColumn(name="co_cargo", referencedColumnName="co_cargo")
     * })
     */
    private $coCargo;

    /**
     * @var \Pessoa
     *
     * @ORM\ManyToOne(targetEntity="Pessoa")
     * @ORM\JoinColumns({
     *   @ORM\JoinColumn(name="co_pessoa", referencedColumnName="co_pessoa")
     * })
     */
    private $coPessoa;


}
