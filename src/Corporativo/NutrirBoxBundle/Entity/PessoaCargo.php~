<?php

namespace Corporativo\ConcurseiroBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * PessoaCargo
 *
 * @ORM\Table(name="pessoa_cargo", uniqueConstraints={@ORM\UniqueConstraint(name="idx_pk_pessoa_cargo", columns={"co_pessoa_cargo"})}, indexes={@ORM\Index(name="idx_fk_cargo_pessoa_cargo", columns={"co_cargo"}), @ORM\Index(name="idx_fk_transacao_pessoa_cargo", columns={"co_transacao"}), @ORM\Index(name="idx_fk_pessoa_pessoa_cargo", columns={"co_pessoa"})})
 * @ORM\Entity
 */
class PessoaCargo
{
    /**
     * @var integer
     *
     * @ORM\Column(name="co_pessoa_cargo", type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="SEQUENCE")
     * @ORM\SequenceGenerator(sequenceName="pessoa_cargo_co_pessoa_cargo_seq", allocationSize=1, initialValue=1)
     */
    private $coPessoaCargo;

    /**
     * @var \Corporativo\ConcurseiroBundle\Entity\Pessoa
     *
     * @ORM\ManyToOne(targetEntity="Corporativo\ConcurseiroBundle\Entity\Pessoa")
     * @ORM\JoinColumns({
     *   @ORM\JoinColumn(name="co_pessoa", referencedColumnName="co_pessoa")
     * })
     */
    private $coPessoa;

    /**
     * @var \Corporativo\ConcurseiroBundle\Entity\Cargo
     *
     * @ORM\ManyToOne(targetEntity="Corporativo\ConcurseiroBundle\Entity\Cargo")
     * @ORM\JoinColumns({
     *   @ORM\JoinColumn(name="co_cargo", referencedColumnName="co_cargo")
     * })
     */
    private $coCargo;

    /**
     * @var \Corporativo\ConcurseiroBundle\Entity\Transacao
     *
     * @ORM\ManyToOne(targetEntity="Corporativo\ConcurseiroBundle\Entity\Transacao")
     * @ORM\JoinColumns({
     *   @ORM\JoinColumn(name="co_transacao", referencedColumnName="co_transacao")
     * })
     */
    private $coTransacao;


}
